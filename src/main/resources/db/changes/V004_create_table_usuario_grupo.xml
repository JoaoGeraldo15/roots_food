    <changeSet id="231220221643-2" author="joao.geraldo">
        <createTable tableName="USUARIO_GRUPO">
            <column name="ID_USUARIO" type="INT" remarks="Chave estrangeira da tabela USUARIO">
                <constraints nullable="false" foreignKeyName="ID_USUARIO_UG_FK" references="USUARIO(ID)"/>
            </column>
            <column name="ID_GRUPO" type="INT" remarks="Chave estrangeira da tabela GRUPO">
                <constraints nullable="false" foreignKeyName="ID_GRUPO_UG_FK" references="GRUPO(ID)"/>
            </column>
        </createTable>
    </changeSet>


</databaseChangeLog>