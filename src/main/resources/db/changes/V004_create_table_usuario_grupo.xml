    <changeSet id="231220221644-0" author="joao.geraldo">
        <dropForeignKeyConstraint baseTableName="USUARIO" constraintName="GRUPO_USUARIO_FK"/>
    </changeSet>

    <changeSet id="231220221644-1" author="joao.geraldo">
        <dropColumn tableName="USUARIO" columnName="ID_GRUPO"/>
